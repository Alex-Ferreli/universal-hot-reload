{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": [
    "babel",
    "import"
  ],
  "rules": {
    "no-return-assign": 0,
    "eol-last": 0,
    "no-plusplus": 0,
    "global-require": 0,
    "arrow-parens": 0,
    "arrow-body-style": 0,
    "consistent-return": 0,
    "indent": [2, 2, {"SwitchCase": 1}],
    "max-len": 0,
    "no-param-reassign": 0,
    "new-cap": 0,
    "no-console": 0,
    "object-curly-spacing": 0,
    "spaced-comment": 0,
    "import/no-extraneous-dependencies": 0,
    "import/imports-first": 0,
    "import/prefer-default-export": 0,
    "no-trailing-spaces": 0,
    "no-underscore-dangle": 0,
    "no-use-before-define": 0,
    "guard-for-in": 0,
    "no-restricted-syntax": 0,
    "no-unused-expressions": 0,
    "one-var": 0
  },
  "env": {
    "jest": true
  },
  "globals": {
    "ldClient": true,
    "window": true,
    "expect": true
  }
}
